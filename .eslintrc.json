{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "import/no-extraneous-dependencies": "off",
    "no-use-before-define": "off",
    "class-methods-use-this": "off",
    "no-unused-vars": "warn",
    "no-lonely-if": "warn",
    "no-param-reassign": "warn"
  }
}
